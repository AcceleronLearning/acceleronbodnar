{
	"name": "acceleronlearning.com",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"deploy": "node ./gh-pages.js"
	},
	"devDependencies": {
		"@builder.io/sdk-svelte": "latest",
		"@playwright/test": "^1.34.3",
		"@sveltejs/adapter-auto": "latest",
		"@sveltejs/adapter-cloudflare": "^2.2.0",
		"@sveltejs/adapter-static": "latest",
		"@sveltejs/kit": "latest",
		"@typescript-eslint/eslint-plugin": "^5.59.8",
		"@typescript-eslint/parser": "^5.59.8",
		"cypress": "latest",
		"eslint": "latest",
		"eslint-config-prettier": "latest",
		"eslint-plugin-svelte3": "latest",
		"gh-pages": "^5.0.0",
		"prettier": "latest",
		"prettier-plugin-svelte": "latest",
		"rollup": "latest",
		"svelte": "latest",
		"svelte-adapter-deno": "^0.9.0",
		"svelte-check": "^3.4.3",
		"tslib": "^2.5.2",
		"typescript": "^5.0.4",
		"vite": "latest",
		"vite-plugin-iso-import": "^1.0.0",
		"vitest": "^0.25.8"
	},
	"type": "module",
	"dependencies": {
		"@builder.io/sdk-svelte": "latest",
		"jquery": "^3.7.0",
		"svelte-preprocess": "^5.0.4"
	},
	"bundledDependencies": false
